{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Build C Program",
			"type": "shell",
			"command": "gcc",
			"args": [
				"-g",
				"-o",
				"${workspaceFolder}/a.exe",
				"${workspaceFolder}/main.c",
				"${workspaceFolder}/ArrayList.c"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"problemMatcher": ["$gcc"]
		}
	]
}
