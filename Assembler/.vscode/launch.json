{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug C Program",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/a.exe",
			"args": ["test.tk", "output.txt", "output.tko"],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"miDebuggerPath": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "Build C Program"
		}
	]
}
